# FinFlow Environment Variables
# Copy this file to .env and fill in your actual values

# InstantDB Configuration
# Get your app ID from https://instantdb.com/dash
VITE_INSTANT_APP_ID=your_instant_app_id_here
INSTANT_APP_ADMIN_TOKEN=your_instant_app_admin_token_here
# Google OAuth Configuration
# Get these from https://console.developers.google.com/
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
VITE_GOOGLE_CLIENT_NAME=your_google_client_name_here

# Example values (replace with your actual values):
# VITE_INSTANT_APP_ID=abc123def456
# VITE_GOOGLE_CLIENT_ID=123456789-abcdefghijklmnop.apps.googleusercontent.com
# VITE_GOOGLE_CLIENT_NAME=FinFlow
