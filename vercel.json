{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/terms",
            "destination": "/terms"
        },
        {
            "source": "/privacy",
            "destination": "/privacy"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}